{
  "name": "job-tracker",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently -k -r \"vite\" \"tsc -p tsconfig.main.json --watch\" \"wait-on http://localhost:5173 && electronmon dist-electron/main/index.js\"",
    "start": "npm run build && electron dist-electron/main/index.js",
    "build": "vite build && tsc -p tsconfig.main.json",
    "postinstall": "electron-rebuild",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write './**/*.{ts,tsx,js,jsx,json,css,md}'",
    "clean:linux": "rm -rf dist-electron && npm run build",
    "clean:win": "powershell -Command \"Remove-Item -Path .\\dist-electron -Recurse -Force\" && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
